<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "https://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<settings>
		<setting name="mapUnderscoreToCamelCase" value="true" />
	</settings>

	<typeAliases>
		<typeAlias
			type="com.douzone.smartlogistics.vo.ProductVo" alias="productvo" />
			<typeAlias type="com.douzone.smartlogistics.vo.BusinessVo" alias="businessvo" />
			<typeAlias type="com.douzone.smartlogistics.vo.UserVo" alias="uservo" />
			<typeAlias type="com.douzone.smartlogistics.vo.ReceiveMasterVo" alias="receivemastervo" />
			<typeAlias type="com.douzone.smartlogistics.vo.ReceiveDetailVo" alias="receivedetailvo" />
	</typeAliases>

	<mappers>
		<mapper resource="mybatis/mappers/product.xml" />
		<mapper resource="mybatis/mappers/business.xml" />
		<mapper resource="mybatis/mappers/user.xml" />
		<mapper resource="mybatis/mappers/receive.xml" />
	</mappers>
</configuration>
